{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Recaptcha": {
    "ApiUrl": "https://www.google.com/recaptcha/api/siteverify",
    "V2": {
      "SiteKey": "YOUR_RECAPTCHA_V2_SITE_KEY",
      "SecretKey": "YOUR_RECAPTCHA_V2_SECRET_KEY"
    },
    "V3": {
      "SiteKey": "YOUR_RECAPTCHA_V3_SITE_KEY",
      "SecretKey": "YOUR_RECAPTCHA_V3_SECRET_KEY",
      "ScoreThreshold": 0.7
    }
  }
}